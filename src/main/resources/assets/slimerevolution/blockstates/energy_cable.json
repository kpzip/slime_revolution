{
    "multipart": [
    {
      "apply": {
        "model": "slimerevolution:block/energy_cable_center_slime"
      }
    },
    {
      "when": {
        "has_north": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x" : 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "has_east": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "z": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "has_south": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "has_west": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "z": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "has_down": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "has_up": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "uvlock": true
      }
    }
  ]
}