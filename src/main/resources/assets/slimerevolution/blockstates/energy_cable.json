{
    "multipart": [
    {
      "apply": {
        "model": "slimerevolution:block/energy_cable_center_slime"
      }
    },
    {
      "when": {
        "has_north": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x" : 90
      }
    },
    {
      "when": {
        "has_east": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x": 270,
        "y": 270
      }
    },
    {
      "when": {
        "has_south": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x": 270
      }
    },
    {
      "when": {
        "has_west": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x": 90,
        "y": 270
      }
    },
    {
      "when": {
        "has_down": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime",
        "x": 180
      }
    },
    {
      "when": {
        "has_up": "true"
      },
      "apply": {
        "model": "slimerevolution:block/energy_cable_outcrop_slime"
      }
    }
  ]
}